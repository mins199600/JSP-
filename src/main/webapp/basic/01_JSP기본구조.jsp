	<%-- JSP Directive(지시자) 
	
			<%@ page %>
			page language="java" : 지금 페이지 언어(생략가능)
			contentType="text/html; : 현재 페이지 문서 타입(필수요소)
			pageEncoding="UTF-8"%(생략가능)
			import = "" 자바 패키지 및 자바 클래스 선언
			
			<%@ include %>
			<%@ taglib %>
			
			jsp 에서 사용가능한 문법 : html, css, js
			
			jsp 코드 작성 영역 선언후 jsp 사용 가능
			<% //코드 적기 %>(= 스크립트릿)
		
	 --%>
